{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact", "typescript.tsx"],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "git.addGlameToVirtualText": true,
  "coc.preferences.formatOnSaveFiletypes": ["css", "typescript", "typescriptreact", "javascript", "javascriptreact"],
  "prettier.tabWidth": 4,
  "prettier.singleQuote": true,
  "prettier.jsxBracketSameLine": true,
  "prettier.trailingComma": "es5",
  "languageserver": {
      "intelephense": {
          "command": "intelephense",
          "args": ["--stdio"],
          "filetypes": ["php"],
          "initializationOptions": {
              "storagePath": "/tmp/intelephense"
          }
      }
  }
}

